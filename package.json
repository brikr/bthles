{
  "name": "bthles",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "update": "npm i",
    "update-functions": "cd functions && npm i && cd -",
    "update-all": "npm run update && npm run update-functions",
    "build-functions": "cd functions && npm run build && cd -",
    "deploy": "npm run deploy-staging",
    "deploy-staging": "npm run update-all && npm run build-functions && npm run build -- -c staging && firebase deploy -P staging",
    "deploy-prod": "npm run update-all && npm run build-functions && npm run build -- -c prod && firebase deploy -P prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.5",
    "@angular/cdk": "^8.1.3",
    "@angular/common": "~10.1.5",
    "@angular/compiler": "~10.1.5",
    "@angular/core": "~10.1.5",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~10.1.5",
    "@angular/material": "^8.1.3",
    "@angular/platform-browser": "~10.1.5",
    "@angular/platform-browser-dynamic": "~10.1.5",
    "@angular/router": "~10.1.5",
    "core-js": "^2.5.4",
    "firebase": "^5.8.5",
    "ngx-clipboard": "^12.2.0",
    "rollbar": "^2.12.0",
    "rxjs": "~6.6.3",
    "tslib": "^1.9.0",
    "validator": "^10.11.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.5",
    "@angular/cli": "~10.1.5",
    "@angular/compiler-cli": "~10.1.5",
    "@angular/language-service": "~10.1.5",
    "@types/node": "~8.9.4",
    "@types/validator": "^10.9.0",
    "clang-format": "^1.2.4",
    "codelyzer": "^5.0.1",
    "gts": "^0.9.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~4.0.3"
  }
}